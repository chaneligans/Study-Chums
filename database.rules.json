{
  /* Visit https://firebase.google.com/docs/database/security to learn more about security rules. */
  "rules": {
    "Users": {
      ".read": true,
      ".indexOn": "index",
      "$uid": {
        ".write": "$uid === auth.uid"
      }
    },
    "TotalUsers": {
      ".read": true,
      "$uid": {
        ".write": "$uid === auth.uid"
      }
    }
  }
}